<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    xmlns:aui="http://schemas.android.com/apk/res/com.alipay.mobile.antui"
    android:layout_height="match_parent">

    <com.alipay.mobile.antui.basic.AULinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.alipay.mobile.antui.viewdemo.view.DemoTitleBar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/title_bar"
            aui:titleText="支付成功" />
        <com.alipay.mobile.antui.viewdemo.view.BaseHeadView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            aui:titleText="Result" />


        <com.alipay.mobile.antui.basic.AUButton
            style="@com.alipay.mobile.antui:style/subButtonStyle"
            android:layout_margin="12dp"
            android:id="@+id/success_action"
            android:clickable="true"
            android:text="操作成功结果页" />
        <com.alipay.mobile.antui.basic.AUButton
            style="@com.alipay.mobile.antui:style/subButtonStyle"
            android:layout_margin="12dp"
            android:clickable="true"
            android:id="@+id/fail_action"
            android:text="操作失败结果页" />

    </com.alipay.mobile.antui.basic.AULinearLayout>

    <com.alipay.mobile.antui.viewdemo.view.BaseFootView
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</RelativeLayout>