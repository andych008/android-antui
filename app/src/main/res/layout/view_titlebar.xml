<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aui="http://schemas.android.com/apk/res/com.alipay.mobile.antui"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!-- AUTitleBar start -->
    <com.alipay.mobile.antui.basic.AUTitleBar
        android:id="@+id/title_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        aui:titleText="主标题导航"
        aui:rightIconUnicode="@com.alipay.mobile.antui:string/iconfont_more" />

    <com.alipay.mobile.antui.basic.AUTitleBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        aui:titleText="标题导航"
        aui:leftIconUnicode="@com.alipay.mobile.antui:string/iconfont_more"
        android:visibility="gone"/>

    <com.alipay.mobile.antui.basic.AUTitleBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        aui:titleText="标题"
        aui:titleTextSize="@com.alipay.mobile.antui:dimen/AU_TEXTSIZE2"
        aui:titleTextColor="#f64219"
        aui:leftIconUnicode="@com.alipay.mobile.antui:string/iconfont_add"
        aui:rightIconUnicode="@com.alipay.mobile.antui:string/iconfont_search"/>

    <com.alipay.mobile.antui.basic.AUTitleBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        aui:titleText="标题2"
        aui:leftIconUnicode="@com.alipay.mobile.antui:string/iconfont_contacts"
        aui:rightIconUnicode="@com.alipay.mobile.antui:string/iconfont_more"/>

    <com.alipay.mobile.antui.basic.AUTitleBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        aui:titleText="标题3"
        aui:leftIconUnicode="@com.alipay.mobile.antui:string/iconfont_bill"
        aui:rightIconUnicode="@com.alipay.mobile.antui:string/iconfont_user"/>

    <com.alipay.mobile.antui.basic.AUTitleBar
        android:id="@+id/title_bar_6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        aui:titleText="标题4"
        aui:leftIconUnicode="@com.alipay.mobile.antui:string/iconfont_user_setting"
        aui:rightText="Setting"/>

    <com.alipay.mobile.antui.basic.AUTitleBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        aui:titleText="标题5"
        aui:backgroundDrawable="@com.alipay.mobile.antui:color/AU_COLOR_HOME_PAGE_TITLEBAR_BG"
        aui:titleTextColor="@color/AU_COLOR15"
        aui:backIconColor="@color/AU_COLOR15"
        aui:rightIconColor="@color/AU_COLOR15"
        aui:rightIconUnicode="@com.alipay.mobile.antui:string/iconfont_setting"/>

    <!-- AUTitleBar end -->

    <com.alipay.mobile.antui.basic.AUTitleBar
        android:id="@+id/dynamic_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        aui:titleText="标题6" />

    <com.alipay.mobile.antui.basic.AUTitleBar
        android:id="@+id/segment_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"/>

    <com.alipay.mobile.antui.viewdemo.view.BaseFootView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" />
</RelativeLayout>